{
    "name": "ronald-ph/laravel-blockchain",
    "description": "A Laravel package to secure your app's data with immutable blockchain records, cryptographic signatures, audit trails, and tamper detection.",
    "type": "library",
    "keywords": [
        "laravel-package",
        "digital-signature",
        "immutable-database",
        "data-integrity",
        "audit-trail",
        "blockchain-ledger",
        "tamper-proof",
        "laravel-security",
        "laravel-blockchain",
        "merkle-verification"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ronald Soberano",
            "email": "ronsoberano02@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|10.0|^11.0|^12.0",
        "illuminate/database": "^9.0|10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "RonaldPH\\LaravelBlockchain\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RonaldPH\\LaravelBlockchain\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RonaldPH\\LaravelBlockchain\\BlockchainServiceProvider"
            ],
            "aliases": {
                "Blockchain": "RonaldPH\\LaravelBlockchain\\Facades\\Blockchain"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "version": "2.0.3"
}
